
import org.jocean.cli.CliCommand;

CliCommand startcmd = _clictx.getCommandRepository().findCommandByAction("startapp");

if (null != startcmd) {
    _LOG.info("found startapp {}", startcmd);

    startcmd.execute(_clictx, new String[]{endpoint, namespace, rolename, config});
}
else {
    _LOG.info("NOT found startapp");
}

